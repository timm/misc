@include "lib"
@include "tab"

function csv1(s,    f,d,a) {
  d = Gold.dot 
  f = d d "/data/" "weather" d "csv"
  while(csv(a, f)) o(a)
}

function tab0(s,rows,f,  d,t) {
  d = Gold.dot 
  Tab(t)
  TabRead(t,d d "/data/" f d "csv")
  ok(s,rows,length(t.rows))
}
function tab1(i,t) { print(10); tab0(i,14,  "weather")}
function tab2(i,t) { tab0(i,398, "auto93" )}
BEGIN{ 
  tests("csv1")
  tests("tab2")
}


