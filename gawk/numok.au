@include "lib"
@include "num"

function _num(f,   n,lst,s,i) {
   s=" 9 2 5 4 12 7 8 11 9 3" \
     " 7 4 12 5 4 10 9 6 9 4"
   split(s, lst, " ")
   Num(n)
   for(i in lst)
     NumAdd(n,lst[i])
   is(f, int(100*n.sd), 306)
   is(f, n.mu, 7)
}

BEGIN {
   srand(1)
   tests("num","_num")
}
