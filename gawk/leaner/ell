alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias gp="git add *;git commit -am save;git push;git status"
alias grep='grep --color=auto'
alias ls="ls -G"

l() {
  f=$1
  shift
  lua $f.lua "$@"
}

eg() {
  f=$1
  shift
  echo "#lua eg$f.lua \"$@\""
  lua eg$f.lua "$@"
}

command_not_found_handle() { l "$@"; }
